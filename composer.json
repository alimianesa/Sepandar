{
    "name": "alimianesa/sepandar",
    "description": "Sepandar Billing helper",
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Salimian",
            "email": "ali.salimiansas2@gmail.com",
            "homepage": "https://github.com/alimianesa"
        }
    ],
    "homepage": "https://github.com/alimianesa/sepandar",
    "keywords": ["Laravel", "Sepandar"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Alimianesa\\Sepandar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alimianesa\\Sepandar\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alimianesa\\Sepandar\\SepandarServiceProvider"
            ],
            "aliases": {
                "Sepandar": "Alimianesa\\Sepandar\\Facades\\Sepandar"
            }
        }
    }
}
